dropdownButton.MouseButton1Click:Connect(function()
	if debounce == false then debounce = true
		isEnabled1 = not isEnabled1
		if isEnabled1 then
			tweenService:Create(dropdownButton, TweenInfo.new(.15, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, false, 0), {ImageTransparency = 1}):Play()
			tweenService:Create(main, predefinedInfo, {Size = UDim2.new(0, 250, 0, 70)}):Play()
			task.wait(.5)
			dropdownButton.Image = "rbxassetid://13013096229"
			colorButton.Visible = true
			settingButton.Visible = true
			dropdownButton.Position = UDim2.new(0.36, 0, 0, 0)
			colors.Position = UDim2.new(0.024, 0, 5.324, 0)
			mainSettings.Position = UDim2.new(0.965, 0, 5.309, 0)
			tweenService:Create(dropdownButton, predefinedInfo, {ImageTransparency = 0}):Play()
			tweenService:Create(colorButton, predefinedInfo, {ImageTransparency = 0}):Play()
			tweenService:Create(settingButton, predefinedInfo, {ImageTransparency = 0}):Play()
			task.wait(.35)
			credits.Visible =  true
			tweenService:Create(credits, predefinedInfo, {BackgroundTransparency = 0.5}):Play()
			tweenService:Create(doNotSteal, predefinedInfo, {TextTransparency = 0}):Play()
			task.wait(1)
			tweenService:Create(credits, predefinedInfo, {Size = UDim2.new(0, 450, 0, 130)}):Play()
			tweenService:Create(credits, predefinedInfo, {Position = UDim2.new(0.5, 0, 2.154, 0)}):Play()
			tweenService:Create(roundy_5, predefinedInfo, {CornerRadius = UDim.new(0, 25)}):Play()
			task.wait(.25)
			tweenService:Create(notice, predefinedInfo, {TextTransparency = 0}):Play()
		else
			tweenService:Create(credits, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(doNotSteal, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(notice, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(main, predefinedInfo, {Size = UDim2.new(0, 70, 0, 70)}):Play()
			tweenService:Create(credits, predefinedInfo, {Size = UDim2.new(0, 450, 0, 70)}):Play()
			tweenService:Create(credits, predefinedInfo, {Position = UDim2.new(0.5, 0, 1.722, 0)}):Play()
			tweenService:Create(roundy_5, predefinedInfo, {CornerRadius = UDim.new(0, 45)}):Play()
			tweenService:Create(colors, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(title_2, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(mainSettings, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(title, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(smoothness, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(smoothness, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(value, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(value, predefinedInfo, {TextTransparency = 1}):Play()
			for _, button in pairs(content:GetChildren()) do
				if button:IsA("TextButton") then
					tweenService:Create(button, predefinedInfo, {BackgroundTransparency = 1}):Play()
					tweenService:Create(button, predefinedInfo, {TextTransparency = 1}):Play()
				end
			end
			tweenService:Create(dropdownButton, TweenInfo.new(.15, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, false, 0), {ImageTransparency = 1}):Play()
			tweenService:Create(colorButton, TweenInfo.new(.15, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, false, 0), {ImageTransparency = 1}):Play()
			tweenService:Create(settingButton, TweenInfo.new(.15, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, false, 0), {ImageTransparency = 1}):Play()
			task.wait(.35)
			dropdownButton.Image = "rbxassetid://13013097088"
			colorButton.Visible = false
			settingButton.Visible = false
			credits.Visible = false
			mainSettings.Visible = false
			colors.Visible = false
			dropdownButton.Position = UDim2.new(0, 0, 0, 0)
			tweenService:Create(dropdownButton, predefinedInfo, {ImageTransparency = 0}):Play()
		end	
		task.wait(timeout) debounce = false
	end
end)

colorButton.MouseButton1Click:Connect(function()
	if debounce2 == false then debounce2 = true
		isEnabled2 = not isEnabled2
		if isEnabled2 then
			tweenService:Create(notice, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(credits, predefinedInfo, {Size = UDim2.new(0, 450, 0, 70)}):Play()
			tweenService:Create(credits, predefinedInfo, {Position = UDim2.new(0.5, 0, 1.722, 0)}):Play()
			tweenService:Create(roundy_5, predefinedInfo, {CornerRadius = UDim.new(0, 45)}):Play()
			colors.Visible = true
			tweenService:Create(colors, predefinedInfo, {BackgroundTransparency = 0.5}):Play()
			tweenService:Create(title_2, predefinedInfo, {TextTransparency = 0}):Play()
			for _, button in pairs(content:GetChildren()) do
				if button:IsA("TextButton") then
					tweenService:Create(button, predefinedInfo, {BackgroundTransparency = 0}):Play()
					tweenService:Create(button, predefinedInfo, {TextTransparency = 0}):Play()
				end
			end
		else
			tweenService:Create(colors, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(title_2, predefinedInfo, {TextTransparency = 1}):Play()
			for _, button in pairs(content:GetChildren()) do
				if button:IsA("TextButton") then
					tweenService:Create(button, predefinedInfo, {BackgroundTransparency = 1}):Play()
					tweenService:Create(button, predefinedInfo, {TextTransparency = 1}):Play()
				end
			end
			task.wait(.5)
			colors.Visible = false
		end
		task.wait(timeout2) debounce2 = false
	end
end)

settingButton.MouseButton1Click:Connect(function()
	if debounce3 == false then debounce3 = true
		isEnabled3 = not isEnabled3
		if isEnabled3 then
			tweenService:Create(notice, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(credits, predefinedInfo, {Size = UDim2.new(0, 450, 0, 70)}):Play()
			tweenService:Create(credits, predefinedInfo, {Position = UDim2.new(0.5, 0, 1.722, 0)}):Play()
			tweenService:Create(roundy_5, predefinedInfo, {CornerRadius = UDim.new(0, 45)}):Play()
			mainSettings.Visible = true
			tweenService:Create(mainSettings, predefinedInfo, {BackgroundTransparency = 0.5}):Play()
			tweenService:Create(title, predefinedInfo, {TextTransparency = 0}):Play()
			tweenService:Create(smoothness, predefinedInfo, {BackgroundTransparency = 0}):Play()
			tweenService:Create(smoothness, predefinedInfo, {TextTransparency = 0}):Play()
			tweenService:Create(value, predefinedInfo, {BackgroundTransparency = 0}):Play()
			tweenService:Create(value, predefinedInfo, {TextTransparency = 0}):Play()
		else
			tweenService:Create(mainSettings, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(title, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(smoothness, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(smoothness, predefinedInfo, {TextTransparency = 1}):Play()
			tweenService:Create(value, predefinedInfo, {BackgroundTransparency = 1}):Play()
			tweenService:Create(value, predefinedInfo, {TextTransparency = 1}):Play()
			task.wait(.5)
			mainSettings.Visible = false
		end
		task.wait(timeout2) debounce3 = false
	end
end)

smoothness.MouseButton1Click:Connect(function()
	isEnabled4 = not isEnabled4
	if isEnabled4 then
		isSmoothEnabled = true
		tweenService:Create(smoothness, predefinedInfo, {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
	else
		isSmoothEnabled = false
		tweenService:Create(smoothness, predefinedInfo, {TextColor3 = Color3.fromRGB(135, 135, 135)}):Play()
	end
end)

value.FocusLost:Connect(function()
	smoothTime = tonumber(value.Text)
end)

for _, button in pairs(content:GetChildren()) do
	if button:IsA("TextButton") then
		button.MouseButton1Click:Connect(function()
			if isSmoothEnabled then
				for _, part in pairs(workspace:GetDescendants()) do
					if part:IsA("BasePart") and part.Name == "Part" then
						tweenService:Create(part, TweenInfo.new(smoothTime, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, false, 0), {Color = button.BackgroundColor3}):Play()
					end
				end
			else
				for _, part in pairs(workspace:GetDescendants()) do
					if part:IsA("BasePart") and part.Name == "Part" then
						tweenService:Create(part, TweenInfo.new(0, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, false, 0), {Color = button.BackgroundColor3}):Play()
					end
				end
			end
		end)
	end
end
